{% extends "shared/short_layout.twig" %}

{% block content %}
    <h1>Viết nhãm xíu</h1>
    <div class="">
        <div class="blog-list">
            {% for post in posts %}
            <div class="blog-item">
                <h3><a href="{{ post.url }}">{{ post.title }}</a></h3>
            </div>
            {% endfor %}
        </div>
    </div>
{% endblock %}

{% block title %}Short{% endblock %}